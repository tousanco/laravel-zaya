{
    "name": "tousanco/laravel-zaya",
    "description": "using this package you can easily implement the zaya.io link shortener in your laravel application.",
    "keywords": [
        "zaya",
        "link shortener"
    ],
    "homepage": "https://github.com/tousanco/laravel-zaya",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "alikhosravi",
            "email": "alikhosravi175@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.*|6.*|7.*|8.*|9.*|10.*",
        "tousanco/php-zaya": "^1.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Tousanco\\LaravelZaya\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tousanco\\LaravelZaya\\LaravelZayaServiceProvider"
            ]
        }
    }
}
